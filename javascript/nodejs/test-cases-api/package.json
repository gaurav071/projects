{
  "name": "test-cases-api",
  "version": "1.0.0",
  "description": "REST APIs to read and maintain test cases in a folder tree structure",
  "main": "app.js",
  "scripts": {
    "transpile": "babel app -d ./dist/app/ && babel config -d ./dist/config && babel lib -d ./dist/lib && babel routes -d ./dist/routes && babel middlewares -d ./dist/middlewares && babel app.js -d ./dist",
    "copy": "cp package.json ./dist",
    "build": "npm run transpile && npm run copy",
    "local": "babel-watch app.js",
    "start": "nodemon dist/app.js",
    "test": "jest",
    "debug": "babel-watch -I app.js"
  },
  "author": "Gaurav Pandey",
  "license": "ISC",
  "dependencies": {
    "@types/jest": "^23.3.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "log4js": "^3.0.5",
    "mongoose": "^5.2.14",
    "mongoose-unique-validator": "^2.0.1",
    "nodemon": "^1.18.4",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-watch": "^2.0.7",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-class-property": "^1.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0"
  }
}
